{
  "extends": "@tsconfig/react-native/tsconfig.json" /* Recommended React Native TSConfig base */,
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@context/*": [
        "context/*"
      ],
      "@screens/*": [
        "screens/*"
      ],
      "@services/*": [
        "services/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    }
  },
  "include": [
    "./**/*",
    "./*",
    ".eslintrc.js"
  ]
}